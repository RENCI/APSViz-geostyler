  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RENCI/APSViz-geostyler.git"
  },
  "keywords": [
    "geo",
    "styler",
    "geostyler"
